<map proto="1.4.0">

<name>Ration</name>
<version>1.0.0</version>
<objective>Venture onto the enemy team's side to collect their wool and bring it back to base!</objective>
<authors>
    <author uuid="af97fa7f-6004-413d-951b-e2e92f5a1f81" contribution="XML and Main Map Building"/>
    <author uuid="d697ff2d-7741-4382-a3df-96b3827b255c" contribution="Suggestions"/>
</authors>
<contributors>
	<author uuid="17feaec5-9616-49d8-a507-83026050dd8f" contribution="Hosting MapDev server"/>
