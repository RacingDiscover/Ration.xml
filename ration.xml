<teams>
	<team id="cyan-team" color="cyan" max="30">Cyan Team</team>
	<team id="magenta-team" color="magenta" max="30">Magenta Team</team>
</teams>
<spawns>	
	<spawn team="cyan-team" kit="cyan">
		<point yaw="0.3">
			<cylinder base="11.517,25.06250,154.600" height="0" radius="2"/>
		</point>
	</spawn>
	<spawn team="magenta-team" kit="magenta">
		<point yaw="0.3">
			<cylinder base="11.409,25.06250,-33.685" height="0" radius="2"/>
			</point>
		</spawn>
		<default yaw="0.3"><cuboid min="14.493,145,56.484" max="8.485,145,60.442"/></default>
	</spawns>
	<wools>
		<wool team="magenta" color="orange">
			<monument><block>14,26,150</block></monument>
		</wool>
		<wool team="magenta" color="purple">
			<monument><block>8,26,150</block></monument>
		</wool>
		<wool team="cyan" color="yellow">
			<monument><block>14,26,-29</block></monument>
		</wool>
		<wool team="cyan" color="lime">
			<monument><block>8,26,-29</block></monument>
		</wool>
	</wools>
			







